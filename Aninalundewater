-- Criando a aba "âš” Batalha"
local BattleTab = Window:MakeTab({
    Name = "âš” Batalha",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})
-- Adicionando o toggle "ðŸ—¡ Mata todo Mundo" na aba "âš” Batalha"
local toggleAttack = false
BattleTab:AddToggle({
    Name = "ðŸ—¡ Matar todo Mundo",
    Default = false,
    Callback = function(Value)
        toggleAttack = Value
        if toggleAttack then
            _G.Stop = false
            local maxDistance = 1000000  -- DistÃ¢ncia mÃ¡xima para considerar jogadores prÃ³ximos (em studs)

            -- FunÃ§Ã£o para selecionar um jogador aleatÃ³rio
            local function getRandomPlayer()
                local players = game:GetService("Players"):GetPlayers()
                local localPlayer = game.Players.LocalPlayer

                -- Remove o jogador local da lista
                local otherPlayers = {}
                for _, player in pairs(players) do
                    if player ~= localPlayer then
                        table.insert(otherPlayers, player)
                    end
                end

                if #otherPlayers == 0 then
                    return nil
                end

                -- Escolhe um jogador aleatÃ³rio da lista
                local randomIndex = math.random(1, #otherPlayers)
                return otherPlayers[randomIndex]
            end

            -- FunÃ§Ã£o para simular a tecla "Q" sendo pressionada
            local function pressQ()
                game:GetService("VirtualInputManager"):SendKeyEvent(true, Enum.KeyCode.Q, false, game)
                wait(0.1)  -- DuraÃ§Ã£o do pressionamento da tecla
                game:GetService("VirtualInputManager"):SendKeyEvent(false, Enum.KeyCode.Q, false, game)
            end

            -- FunÃ§Ã£o principal para atacar e trocar de jogador
            local function attackAndSwitch()
                local localPlayer = game.Players.LocalPlayer
                local localPlayerChar = localPlayer.Character or localPlayer.CharacterAdded:Wait()

                while toggleAttack and not _G.Stop do
                    game:GetService("RunService").RenderStepped:Wait()
                    if not localPlayerChar then continue end

                    -- Seleciona um jogador aleatÃ³rio
                    local targetPlayer = getRandomPlayer()
                    if targetPlayer then
                        local char = targetPlayer.Character or workspace:FindFirstChild(targetPlayer.Name)
                        if char and char:FindFirstChild("Humanoid") and char.Humanoid.Health > 0 then
                            local distance = (localPlayerChar.HumanoidRootPart.Position - char.HumanoidRootPart.Position).Magnitude
                            if distance <= maxDistance then
                                print("Iniciando ataque ao jogador: " .. targetPlayer.Name)
                                localPlayerChar.HumanoidRootPart.CFrame = char.HumanoidRootPart.CFrame
                                -- Simula o pressionamento da tecla "Q"
                                pressQ()
                                -- Aguarda 1 segundo antes de passar para o prÃ³ximo jogador
                                wait(1)
                            end
                        end
                    end
                end
            end

            -- Inicia a funÃ§Ã£o de ataque e troca de jogador
            attackAndSwitch()
        else
            _G.Stop = true
        end
    end
})
-- Adicionando uma opÃ§Ã£o para selecionar o player e atacar
BattleTab:AddTextbox({
    Name = "ðŸ“ Nome do Jogador",
    Default = "",  -- Nome padrÃ£o do jogador deixado em branco
    TextChangedCallback = function(Text)
        selectedPlayerName = Text
    end
})

BattleTab:AddToggle({
    Name = "ðŸ—¡ Atacar Jogador EspecÃ­fico",
    Default = false,
    Callback = function(Value)
        toggleSpecificPlayerAttack = Value
        if toggleSpecificPlayerAttack then
            _G.Stop = false
            local maxDistance = 1000000  -- DistÃ¢ncia mÃ¡xima para considerar jogadores prÃ³ximos (em studs)

            -- FunÃ§Ã£o para encontrar um jogador especÃ­fico
            local function getTargetPlayer(targetName)
                local players = game:GetService("Players"):GetPlayers()
                for _, player in pairs(players) do
                    if player.Name == targetName then
                        return player
                    end
                end
                return nil
            end

            -- FunÃ§Ã£o para simular a tecla "Q" sendo pressionada
            local function pressQ()
                game:GetService("VirtualInputManager"):SendKeyEvent(true, Enum.KeyCode.Q, false, game)
                wait(0.1)  -- DuraÃ§Ã£o do pressionamento da tecla
                game:GetService("VirtualInputManager"):SendKeyEvent(false, Enum.KeyCode.Q, false, game)
            end

            -- FunÃ§Ã£o principal para atacar o jogador especÃ­fico
            local function attackSpecificPlayer()
                local localPlayer = game.Players.LocalPlayer
                local localPlayerChar = localPlayer.Character or localPlayer.CharacterAdded:Wait()

                while toggleSpecificPlayerAttack and not _G.Stop do
                    game:GetService("RunService").RenderStepped:Wait()
                    if not localPlayerChar then continue end

                    -- Encontra o jogador especÃ­fico
                    local targetPlayer = getTargetPlayer(selectedPlayerName)
                    if targetPlayer then
                        local char = targetPlayer.Character or workspace:FindFirstChild(targetPlayer.Name)
                        if char and char:FindFirstChild("Humanoid") and char.Humanoid.Health > 0 then
                            local distance = (localPlayerChar.HumanoidRootPart.Position - char.HumanoidRootPart.Position).Magnitude
                            if distance <= maxDistance then
                                print("Iniciando ataque ao jogador: " .. targetPlayer.Name)
                                localPlayerChar.HumanoidRootPart.CFrame = char.HumanoidRootPart.CFrame
                                -- Simula o pressionamento da tecla "Q"
                                pressQ()
                                -- Aguarda 1 segundo antes de repetir o ataque
                                wait(1)
                            end
                        end
                    end
                end
            end

            -- Inicia a funÃ§Ã£o de ataque ao jogador especÃ­fico
            attackSpecificPlayer()
        else
            _G.Stop = true
        end
    end
})
